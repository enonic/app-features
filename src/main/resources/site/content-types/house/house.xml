<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <display-name>House</display-name>
  <super-type>base:structured</super-type>
  <form>
    <input name="city" type="TextLine">
      <label>City</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="location" type="GeoPoint">
      <label>Location</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="price" type="Long">
      <label>Price</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="number_floor" type="Long">
      <label>Number floors</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="description" type="TextLine">
      <label>Description</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="publish_date" type="Date">
      <label>Date</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
  </form>
</content-type>
